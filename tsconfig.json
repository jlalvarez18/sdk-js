{
  "compilerOptions": {
    "lib": [
      "es6",
      "dom"
    ],
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "strict": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "noImplicitReturns": true
  },
  "compileOnSave": true,
  "include": [
    "./src/**/*"
  ]
}
